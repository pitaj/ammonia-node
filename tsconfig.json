{
  "include": [
    "src/**/*.ts"
  ],
  "compilerOptions": {
    "outDir": "lib",
    "lib": ["es2017"],
    "target": "es2015",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "baseUrl": "",
    "paths": {
      "native": ["./native"]
    }
  },
  "typeAcquisition": {
    "enable": false
  }
}